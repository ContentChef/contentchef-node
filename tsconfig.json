{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "declarationDir": "dist",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015",
      "es2015.collection",
      "es2015.core",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "es2016",
      "es2016.array.include",
      "dom"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "target": "es5"
  },
  "exclude": [
    "node_modules"
  ]
}
